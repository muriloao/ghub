{
    "dart.flutterSdkPath": null,
    "dart.projectSearchDepth": 5,
    "dart.flutterSdkPaths": [],
    "dart.enableSdkFormatter": true,
    "dart.lineLength": 80,
    "editor.formatOnSave": true,
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
